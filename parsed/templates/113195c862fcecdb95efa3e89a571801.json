{"components/blog/post":"<article class=\"blog {{#if show_tags}}{{#if post.tags}}blog--has-tags{{/if}}{{/if}} {{#unless hide_social_icons}}blog--has-socials{{/unless}}\">\n    <div class=\"blog-post-figure\">\n        {{#if post.thumbnail}}\n            <figure class=\"blog-thumbnail\">\n                <a data-instantload href=\"{{post.url}}\">\n                    {{> components/common/responsive-img-url\n                        image=(getImage post.thumbnail 'blog_size')\n                        alt=post.thumbnail.alt}}\n                </a>\n            </figure>\n        {{/if}}\n    </div>\n\n    <div class=\"blog-post-body\">\n        <header class=\"blog-header\">\n            {{#unless single}}\n                <h2 class=\"blog-title\">\n                    <a data-instantload href=\"{{post.url}}\">{{post.title}}</a>\n                </h2>\n            {{/unless}}\n        </header>\n\n        <div class=\"blog-post\">\n            {{#if post.body}}\n                {{{post.body}}}\n                <div class=\"blog-date\"><span class=\"blog-date-date\">{{post.date_published}}{{#if post.author}}</span> <span class=\"blog-date-author\">{{lang 'blog.posted_by' name=post.author}}{{/if}}</span></div>\n            {{else}}\n                {{#unless hide_summary}}\n                    <div class=\"blog-summary\">\n                        {{{post.summary}}}\n                        {{#if post.show_read_more}}\n                            &hellip; \n                        {{/if}}\n                    </div>\n                {{/unless}}\n\n                <div class=\"blog-dateContainer\">\n                    {{#unless hide_readmore}}\n                    {{#if post.show_read_more}}\n                        <div class=\"blog-readmore\"><a data-instantload href=\"{{url}}\">{{lang 'blog.read_more'}}</a></div>\n                    {{/if}}\n                    {{/unless}}\n                    <div class=\"blog-date\"><span class=\"blog-date-date\">{{post.date_published}}{{#if post.author}}</span> <span class=\"blog-date-author\">{{lang 'blog.posted_by' name=post.author}}{{/if}}</span></div>\n                </div>\n            {{/if}}\n        </div>\n\n        {{#if show_tags}}\n            {{#if post.tags}}\n                <ul class=\"tags\">\n                    {{#each post.tags}}\n                    <li class=\"tag\">\n                        <a data-instantload href=\"{{url}}\">#{{name}}</a>\n                    </li>\n                    {{/each}}\n                </ul>\n            {{/if}}\n        {{/if}}\n\n        {{#unless hide_social_icons}}\n            {{> components/common/share url=post.url}}\n        {{/unless}}\n\n    </div>\n</article>\n","components/common/responsive-img-url":"<img\n    {{#if lazyload '==' 'disabled'}}\n        src=\"{{image}}\" \n    {{else}}\n        data-sizes=\"auto\"\n        src=\"data:image/gif;base64,R0lGODlhAQABAIAAAP///wAAACH5BAEAAAAALAAAAAABAAEAAAICRAEAOw==\" \n        data-src=\"{{image}}\"\n    {{/if}}\n    class=\"{{#unless lazyload '==' 'disabled'}}lazyload{{/unless}}{{#if class}} {{class}}{{/if}}\"\n    alt=\"{{alt}}\"\n    title=\"{{alt}}\"\n/>\n","components/common/share":"\n{{#or settings.add_this.buttons settings.facebook_like_button.enabled}}\n    {{assignVar 'encodedUrl' (encodeURI url)}}\n    {{assignVar 'encodedTitle' (encodeURI head.title)}}\n    <div class=\"addthis_toolbox\">\n        {{#if settings.add_this.buttons}}\n            <ul class=\"socialLinks socialLinks--alt\"{{#if idPrefix}} id=\"socialLinks-{{idPrefix}}\"{{/if}}>\n                {{#each settings.add_this.buttons}}\n                    {{#if service '===' 'email'}}\n                        <li class=\"socialLinks-item socialLinks-item--{{service}}\">\n                            <a class=\"socialLinks__link icon icon--{{service}}\"\n                            title=\"{{{capitalize service}}}\"\n                            href=\"mailto:?subject={{getVar 'encodedTitle'}}&amp;body={{getVar 'encodedUrl'}}\"\n                            target=\"_self\"\n                            rel=\"noopener\"\n                            {{{annotation}}}\n                            >\n                                <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\n                                <!-- <svg>\n                                    <use xlink:href=\"#icon-envelope\"/>\n                                </svg> -->\n                                <img alt=\"Email\" src=\"https://cdn11.bigcommerce.com/s-eplfis85ct/product_images/uploaded_images/hex-icon-silver-mail-72px.png\">\n                                \n                            </a>\n                            \n                        </li>\n\n        {{else if service '===' 'print'}}\n        <li class=\"socialLinks-item socialLinks-item--{{service}}\">\n        <a class=\"socialLinks__link icon icon--{{service}}\"\n           title=\"{{{capitalize service}}}\"\n           onclick=\"window.print();return false;\"\n           {{{annotation}}}\n        >\n            <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\n            <!-- <svg>\n                <use xlink:href=\"#icon-print\"/>\n            </svg> -->\n            <img alt=\"Print\" src=\"https://cdn11.bigcommerce.com/s-eplfis85ct/product_images/uploaded_images/printicon.png\">\n        </a>\n        </li>\n\n        {{/if}}\n                {{/each}}\n                {{#each settings.add_this.buttons}}\n                    {{#if service '!=' 'email'}}\n        {{#if service '!=' 'print'}}\n                        <li class=\"socialLinks-item socialLinks-item--{{service}}\">\n                            {{#if service '===' 'facebook'}}\n                            <a class=\"socialLinks__link icon icon--{{service}}\"\n                            title=\"{{{capitalize service}}}\"\n                            href=\"https://facebook.com/sharer/sharer.php?u={{getVar 'encodedUrl'}}\"\n                            target=\"_blank\"\n                            rel=\"noopener\"\n                            {{{annotation}}}\n                            >\n                                <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\n                                <!-- <svg>\n                                    <use xlink:href=\"#icon-facebook\"/>\n                                </svg> -->\n                                <img alt=\"QRFS on Facebook\" title=\"QRFS on Facebook\" src=\"https://cdn11.bigcommerce.com/s-eplfis85ct/product_images/uploaded_images/facebook-72px.png\">\n                            </a>\t\t\t\t\t\t\n                            \n                            {{else if service '===' 'email'}}\n                            <a class=\"socialLinks__link icon icon--{{service}}\"\n                            title=\"{{{capitalize service}}}\"\n                            href=\"mailto:?subject={{getVar 'encodedTitle'}}&amp;body={{getVar 'encodedUrl'}}\"\n                            target=\"_self\"\n                            rel=\"noopener\"\n                            {{{annotation}}}\n                            >\n                                <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\n                                <!-- <svg>\n                                    <use xlink:href=\"#icon-envelope\"/>\n                                </svg> -->\n                                <img alt=\"Email\" title=\"Email\" src=\"https://cdn11.bigcommerce.com/s-eplfis85ct/product_images/uploaded_images/hex-icon-silver-mail-72px.png\">\n                                \n                            </a>\n                            \n                            {{else if service '===' 'twitter'}}\n                            <a class=\"socialLinks__link icon icon--{{service}}\"\n                            href=\"https://twitter.com/intent/tweet/?text={{getVar 'encodedTitle'}}&amp;url={{getVar 'encodedUrl'}}\"\n                            target=\"_blank\"\n                            rel=\"noopener\"\n                            title=\"{{{capitalize service}}}\"\n                            {{{annotation}}}\n                            >\n                                <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\n                                <!-- <svg>\n                                    <use xlink:href=\"#icon-twitter\"/>\n                                </svg> -->\n                                <img alt=\"QRFS on Twitter\" title=\"QRFS on Twitter\" src=\"https://cdn11.bigcommerce.com/s-eplfis85ct/product_images/uploaded_images/twitter-72px.png\">\n                            </a>\t\t\t\t\t\t\n                        \n                          \n                        \n                            {{else if service '===' 'linkedin'}}\n                            <a class=\"socialLinks__link icon icon--{{service}}\"\n                            title=\"{{{capitalize service}}}\"\n                            href=\"https://www.linkedin.com/shareArticle?mini=true&amp;url={{getVar 'encodedUrl'}}&amp;title={{getVar 'encodedTitle'}}&amp;summary={{getVar 'encodedTitle'}}&amp;source={{getVar 'encodedUrl'}}\"\n                            target=\"_blank\"\n                            rel=\"noopener\"\n                            {{{annotation}}}\n                            >\n                                <img alt=\"QRFS on LinkedIn\" title=\"QRFS on LinkedIn\" src=\"https://cdn11.bigcommerce.com/s-eplfis85ct/product_images/uploaded_images/linkedin-72px.png\">\n                            </a>\n                            {{else if service '===' 'pinterest'}}\n                        {{!--  <a class=\"socialLinks__link icon icon--{{service}}\"\n                            title=\"{{{capitalize service}}}\"\n                            href=\"https://pinterest.com/pin/create/button/?url={{getVar 'encodedUrl'}}&amp;description={{getVar 'encodedTitle'}}\"\n                            target=\"_blank\"\n                            rel=\"noopener\"\n                            {{{annotation}}}\n                            >\n                                <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\n                                <svg>\n                                    <use xlink:href=\"#icon-pinterest\"/>\n                                </svg>\n                            </a>\n                            --}}\n                            {{/if}}\n                        </li>\n        {{/if}}\n                    {{/if}}\n                {{/each}}\n            </ul>\n        {{/if}}\n        {{#if settings.facebook_like_button.enabled}}\n            <iframe\n                class=\"facebook{{#if settings.facebook_like_button.verb '===' 'recommend'}}Recommend{{else}}Like{{/if}}Btn\"\n                style=\"border:none; overflow:hidden; margin: 0; padding: 0; height: 20px; margin-top: .75rem; max-width: 100%\"\n                src=\"https://www.facebook.com/plugins/like.php?href={{settings.facebook_like_button.href}}&amp;layout=button_count{{#if settings.facebook_like_button.verb '===' 'recommend'}}&amp;action=recommend{{/if}}&amp;colorscheme=light&amp;height=20\"\n                scrolling=\"no\"\n                frameborder=\"0\"\n                allowTransparency=\"true\">\n            </iframe>\n        {{/if}}\n        <!-- <div class=\"fast-ship-modal-link\">\n            <a href=\"#\" data-reveal-id=\"modal-shipping-description\"><p>{{lang 'products.fast_shipping_and_easy_return'}}</p></a>\n        </div> -->\n    </div>\n{{/or}}\n\n","components/supermarket/sidebar/blog":"{{#if posts}}\n<div class=\"sidebarBlock sidebarBlock--blog\">\n    <h3 class=\"sidebarBlock-heading\">{{lang 'blog.recent_posts'}}</h3>\n    <ul class=\"navList\">\n        {{#each posts}}\n        <li class=\"navList-item\">\n            {{> components/blog/post post=this hide_social_icons=true settings=../settings hide_summary=../hide_summary hide_readmore=../hide_readmore}}\n        </li>\n        {{/each}}\n    </ul>\n</div>\n{{/if}}\n"}
