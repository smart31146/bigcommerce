{"components/beautify/contact-form-remote":"<form data-contact-form class=\"form\" action=\"{{urls.contact_us_submit}}\" method=\"post\">\n    {{#if forms.contact.error}}\n        <div id=\"contact-us-error\">{{> components/common/alert-error forms.contact.error}}</div>\n    {{/if}}\n    <input type=\"hidden\" name=\"page_id\" id=\"page_id\" value=\"{{forms.contact.page_id}}\">\n    \n    {{#if forms.contact.name}}\n        <div class=\"form-field\">\n            <label class=\"form-label\" for=\"contact_fullname\">{{lang 'forms.contact_us.full_name'}}</label>\n            <input class=\"form-input\" type=\"text\" id=\"contact_fullname\" name=\"contact_fullname\" value=\"{{customer.name}}\" placeholder=\"{{lang 'forms.contact_us.full_name'}}\">\n        </div>\n    {{/if}}\n\n    <div class=\"form-field\">\n        <label class=\"form-label\" for=\"contact_email\">{{lang 'forms.contact_us.email'}}\n            <small class=\"_required\" title=\"{{lang 'common.required'}}\">{{lang 'common.required_symbol'}}</small>\n        </label>\n        <input class=\"form-input\" type=\"text\" id=\"contact_email\" name=\"contact_email\" value=\"{{customer.email}}\" placeholder=\"{{lang 'forms.contact_us.email'}}\">\n    </div>\n\n    <div class=\"form-field\">\n        <label class=\"form-label\" for=\"contact_question\">{{lang 'forms.contact_us.question'}}\n            <small class=\"_required\" title=\"{{lang 'common.required'}}\">{{lang 'common.required_symbol'}}</small>\n        </label>\n        <textarea name=\"contact_question\" id=\"contact_question\" rows=\"3\" cols=\"50\" class=\"form-input\" placeholder=\"{{lang 'forms.contact_us.question'}}\"></textarea>\n    </div>\n\n    {{{forms.contact.recaptcha.markup}}}\n\n    <div class=\"form-actions\">\n        <input class=\"button button--primary\" type=\"submit\" value=\"{{lang 'forms.contact_us.submit' }}\">\n    </div>\n</form>\n","components/common/alert-error":"<div class=\"alertBox alertBox--error\">\n    <div class=\"alertBox-column alertBox-icon\">\n        <icon glyph=\"ic-error\" class=\"icon\" aria-hidden=\"true\"><svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\"><path d=\"M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z\"></path></svg></icon>\n    </div>\n    <p class=\"alertBox-column alertBox-message\">\n        <span>{{{this}}}</span>\n    </p>\n</div>\n"}
