{"components/faceted-search/facets/range":"<div class=\"_accordion-block\">\n    <div\n        class=\"_accordion-navigation toggleLink {{#unless ../start_collapsed }} is-open {{/unless}}\"\n        role=\"button\"\n        tabindex=\"0\"\n        data-collapsible=\"#facetedSearch-content--{{dashcase facet }}\">\n        <h4 class=\"_accordion-title\">\n            {{ title }}\n        </h4>\n\n        <div class=\"_accordion-navigation-actions\">\n            {{#if selected}}\n                <a href=\"{{ remove_url }}\" class=\"facetedSearch-clearLink\" data-faceted-search-facet>{{lang 'search.faceted.clear'}}</a>\n            {{/if}}\n            <svg class=\"icon _accordion-indicator toggleLink-text toggleLink-text--off\">\n                <use xlink:href=\"#icon-keyboard-arrow-down\" />\n            </svg>\n            <svg class=\"icon _accordion-indicator toggleLink-text toggleLink-text--on\">\n                <use xlink:href=\"#icon-keyboard-arrow-up\" />\n            </svg>\n        </div>\n    </div>\n\n    <div id=\"facetedSearch-content--{{dashcase facet }}\" class=\"_accordion-content {{#unless ../start_collapsed }} is-open {{/unless}}\">\n        <div class=\"beautify__shopByPrice-slider\" data-slider></div>\n        <form id=\"facet-range-form\" class=\"form\" method=\"get\" data-faceted-search-range novalidate>\n            <input type=\"hidden\" name=\"search_query\" value=\"{{search_query}}\">\n            {{#if this.sort}}\n                <input type=\"hidden\" name=\"sort\" value={{this.sort}}>\n            {{/if}}\n            <fieldset class=\"form-fieldset\">\n                <div class=\"form-minMaxRow\">\n                    <div class=\"form-field form-field--input\">\n                        <input\n                            name=\"min_price\"\n                            placeholder=\"{{lang 'search.faceted.range.min-placeholder'}}\"\n                            min=\"0\"\n                            class=\"form-input form-input--small\"\n                            required\n                            type=\"number\"\n                            value=\"{{ min_price }}\"\n                        /><span class=\"_currency\">{{settings.money.currency_token}}</span>\n                    </div>\n\n                    <div class=\"form-field form-field--input\">\n                        <input\n                            name=\"max_price\"\n                            placeholder=\"{{lang 'search.faceted.range.max-placeholder'}}\"\n                            min=\"0\"\n                            class=\"form-input form-input--small\"\n                            required\n                            type=\"number\"\n                            value=\"{{ max_price }}\"\n                        /><span class=\"_currency\">{{settings.money.currency_token}}</span>\n                    </div>\n\n                    <div class=\"form-field\">\n                        <button class=\"button button--outline button--small\" type=\"submit\">\n                            {{lang 'search.faceted.range.update'}}\n                        </button>\n                    </div>\n                </div>\n\n                <div class=\"form-inlineMessage\"></div>\n            </fieldset>\n        </form>\n        <div class=\"navList-actions-inline\">\n            <button class=\"navList-action\" data-cancel style=\"display:none\">{{lang 'common.cancel'}}</button>\n            <a href=\"{{remove_url}}\" class=\"navList-action\" data-faceted-search-facet data-clear style=\"display:none\">{{lang 'search.faceted.clear'}}</a>\n        </div>\n    </div>\n</div>\n"}
