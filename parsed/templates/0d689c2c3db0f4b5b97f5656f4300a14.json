{"components/common/navigation-page-list":"{{#if children}}\n<a class=\"navPages-action has-subMenu\" href=\"{{url}}\" \n    data-instantload\n    data-collapsible=\"navPages-page-{{@index}}\"\n    data-collapsible-disabled-breakpoint=\"medium\"\n    data-collapsible-disabled-state=\"open\"\n    data-collapsible-enabled-state=\"closed\">\n    {{name}} <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-add\" /></svg></i>\n</a>\n<div class=\"navPage-subMenu navPage-subMenu--standard\" id=\"navPages-page-{{@index}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n        <li class=\"navPage-subMenu-item\">\n            <a class=\"navPage-subMenu-action navPages-action\" data-instantload href=\"{{url}}\">{{name}}</a>\n        </li>\n        {{#each children}}\n            <li class=\"navPage-subMenu-item\">\n                {{#if children}}\n                    <a class=\"navPages-action-toggle\" href=\"#\"\n                        data-collapsible=\"navPages-page-{{../../index}}-{{@index}}\">\n                        <span class=\"_more\">{{lang 'common.nav_more'}} <svg class=\"icon\"><use xlink:href=\"#icon-add\"></use></svg></span>\n                        <span class=\"_less\">{{lang 'common.nav_less'}} <svg class=\"icon\"><use xlink:href=\"#icon-remove\"></use></svg></span>\n                    </a>\n                    <a class=\"navPage-subMenu-action has-subMenu\" href=\"{{url}}\" data-instantload>{{name}}</a>\n                    <ul class=\"navPage-childList\" id=\"navPages-page-{{../../index}}-{{@index}}\">\n                        {{#each children}}\n                        <li class=\"navPage-childList-item\">\n                            <a class=\"navPage-childList-action navPages-action\" data-instantload href=\"{{url}}\">{{name}}</a>\n                        </li>\n                        {{/each}}\n                    </ul>\n                {{else}}\n                    <a class=\"navPage-subMenu-action navPages-action\" data-instantload href=\"{{url}}\">{{name}}</a>\n                {{/if}}\n            </li>\n        {{/each}}\n    </ul>\n</div>\n{{else}}\n<a class=\"navPages-action\" data-instantload href=\"{{url}}\">{{name}}</a>\n{{/if}}\n"}
