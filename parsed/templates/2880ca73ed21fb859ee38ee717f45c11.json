{"components/beautify/navigation-list-child":"{{#if children}}\n    <a class=\"navPages-action-toggle\" href=\"#\"\n        data-collapsible=\"navPages-{{#if page_type '==' 'page'}}page-{{getVar 'navWebPageCount'}}{{/if}}{{id}}{{idSuffix}}\">\n        <span class=\"_more\">{{lang 'common.nav_more'}} <svg class=\"icon\"><use xlink:href=\"#icon-add\"></use></svg></span>\n        <span class=\"_less\">{{lang 'common.nav_less'}} <svg class=\"icon\"><use xlink:href=\"#icon-remove\"></use></svg></span>\n    </a>\n    <a\n        class=\"navPage-childList-action has-subMenu\" \n        href=\"{{url}}\"\n        {{#if menu_type '!=' 'vertical'}}data-instantload{{/if}}\n        {{#if id '==' category_id}}data-current-category{{/if}}\n    >{{name}}</a>\n    <a class=\"navPages-action-toggle-desktop\" href=\"#\" data-collapsible=\"navPages-{{#if page_type '==' 'page'}}page-{{getVar 'navWebPageCount'}}{{/if}}{{id}}{{idSuffix}}\" title=\"{{lang 'common.nav_more'}}\"><svg class=\"icon\"><use xlink:href=\"#icon-caret-down\"></use></svg></a>\n    <ul class=\"navPage-childList\" id=\"navPages-{{#if page_type '==' 'page'}}page-{{getVar 'navWebPageCount'}}{{/if}}{{id}}{{idSuffix}}\">\n        {{#each children}}\n            <!-- {{incrementVar 'navWebPageCount'}} -->\n            <li class=\"navPage-childList-item\">\n                {{> components/beautify/navigation-list-child \n                    category_id=../category_id \n                    idSuffix=../idSuffix \n                    theme_settings=../theme_settings\n                    menu_type=../menu_type}}\n            </li>\n        {{/each}}\n    </ul>\n{{else}}\n    <a class=\"navPage-childList-action\" data-instantload href=\"{{url}}\" {{#if id '==' category_id}}data-current-category{{/if}}>{{name}}</a>\n{{/if}}\n"}
