{"components/common/forms/radio":"<div class=\"form-field\" id=\"{{id}}\" data-validation=\"{{validation}}\">\n    <label class=\"form-label\">{{label}}\n        {{#if required}}\n            <small class=\"_required\" title=\"{{lang 'common.required'}}\">{{lang 'common.required_symbol'}}</small>\n        {{/if}}\n    </label>\n    {{#if radios}}\n        {{#each radios}}\n        <div class=\"radio-wrapper\">\n            <input \n                type=\"radio\"\n                class=\"form-radio\"\n                name=\"{{../name}}\"\n                id=\"{{../name}}_{{@index}}\"\n                value=\"{{value}}\"\n                data-label=\"{{label}}\"\n                aria-required=\"{{../required}}\"\n                {{#if checked}}\n                    checked\n                {{/if}}\n                {{#if private_id}}\n                    data-field-type=\"{{private_id}}\"\n                {{/if}}>\n            <label class=\"form-label\" for=\"{{../name}}_{{@index}}\">{{label}}</label>\n        {{/each}}\n    {{/if}}\n</div>\n</div>\n"}
